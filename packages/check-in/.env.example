# Sanity
SANITY_WRITE_TOKEN=sk...

# Auth salt - for session/authentication hashing (generate with: openssl rand -base64 32)
AUTH_SALT=random-secret-for-auth

# Anonymous salt - for anonymizing check-in data (must be different from AUTH_SALT)
ANONYMOUS_SALT=random-secret-for-anonymous-ids

# JWT secret
JWT_SECRET=random-secret-for-jwt
